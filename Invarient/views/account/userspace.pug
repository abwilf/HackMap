extends ../layout.pug

block content
    #Contacttitle.container-fluid
    h2#Contactword.col-xs-11.col-sm-10.col-md-9.col-lg-8.col-xs-offset-1.col-sm-offset-1.col-md-offset-1.col-lg-offset-1 Maps
    hr
    // Link 1
    .container-fluid
      each val in user.maps
        a(class='userMaps', href=val.url, target="_blank") 
          img(src='css/mat/0076a3.jpg', width='250px', height='150px')
          figcaption #{val.title}